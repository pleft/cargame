void control() {
  //move the car using the buttons
  if (gb.buttons.repeat(BUTTON_RIGHT, 1)) { //every 1 frame when the right button is held down
    carX = carX + carVX; //increase the horizontal position by the car's velocity
  }
  if (gb.buttons.repeat(BUTTON_LEFT, 1)) {
    carX = carX - carVX;
  }
  if (gb.buttons.repeat(BUTTON_DOWN, 1)) {
    carY = carY + carVY;
  }
  if (gb.buttons.repeat(BUTTON_UP, 1)) {
    carY = carY - carVY;
  }

  //check that the car is not going out of the screen
  //if the car is touching the left side of the screen
  if (carX < 0) {
    //bring it back in the screen
    carX = 0;
  }
  //if the car is touching the right side
  if ((carX + carSizeX) > gb.display.width()) {
    carX = gb.display.width() - carSizeX;
  }
  //if the car is touching the top side of the road
  if (carY < roadSize) {
    carY = roadSize;
  }
  //if the car is touching the down side of the road
  if ((carY + carSizeY) > gb.display.height() - roadSize) {
    carY = gb.display.height() - roadSize - carSizeY;
  }
}

void drawTree(int x, int y) {
  gb.display.drawImage(x, y, tree, 8, 8);
}

void drawCar() {
  gb.display.drawImage(carX, carY, car, carSizeX, carSizeY);
}

void drawBackground() {
  gb.display.drawImage(0, 0, background);
}

void animateLanesAndTrees() {
   animationCounter -= animationSpeed;  
  for(int i=0 + animationCounter; i<gb.display.width(); i+=16) {
    gb.display.fillRect(i, gb.display.height()/2 - 1, laneSizeX, laneSizeY);
    drawTree(i*2, 10);
    drawTree(i*3, 40+8);
  }
  if(animationCounter == -16) {
    animationCounter = 0;
  }
}

void setup() {
  gb.begin();
}

void loop() {
  // wait until the gamebuino is ready to update at stable 25 FPS
  // this also updates sounds, button presses....everything!
  while (!gb.update());

  // clear the previous screen
  gb.display.clear();

  // handle car
  control();
  // draw background
  drawBackground();
  // animate the road lanes and the trees
  animateLanesAndTrees();
  // draw the car
  drawCar();
}